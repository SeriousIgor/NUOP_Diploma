<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="328.0" prefWidth="441.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.diploma.controllers.LoginWindowController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <Label fx:id="testLabel" text="Label" />
   <Label alignment="TOP_LEFT" text="Username">
      <VBox.margin>
         <Insets right="250.0" />
      </VBox.margin>
   </Label>
   <TextField fx:id="userName" alignment="CENTER" prefHeight="26.0" prefWidth="338.0">
      <VBox.margin>
         <Insets left="45.0" right="45.0" />
      </VBox.margin>
   </TextField>
   <Label text="Password">
      <VBox.margin>
         <Insets right="255.0" />
      </VBox.margin>
   </Label>
   <TextField fx:id="password" alignment="CENTER" prefHeight="26.0" prefWidth="338.0">
      <VBox.margin>
         <Insets left="45.0" right="45.0" />
      </VBox.margin>
   </TextField>
   <Button fx:id="submitButton" onAction="#onSubmitButtonClick" text="Submit" />
</VBox>
